<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<!-- namespace：命名空间，用于隔离sql-->
<mapper namespace="com.gechuang.mybatis.domain.UserMapper">
    <!-- 代表结果集合对象的映射关系 resultMapping -->
    <resultMap type="" id=""></resultMap>
    <!-- 
       keyClumn:主键对应的列
       keyProperty:主键对应的属性
       useGeneratedKeys:数据自动生成的主键id保存到oid属性当中
     -->
    <insert id="add" keyColumn="" keyProperty="" useGeneratedKeys="true" parameterType="user">
          insert into user(name,email,age,bornDate) values (#{name},#{email},#{age},#{bornDate})
    </insert>
    <update id="update" parameterType="user">
          update user set name = #{name},email=#{email},age=#{age},bornDate=#{bornDate} where id = #{id}
    </update>
    <select id="get" parameterType="long" resultType="user">
         select id,name,email,age,bornDate from user where id = #{id}
    </select>
    <delete id="delete" parameterType="long">
          delete from user where id = #{id}
    </delete>
    
    <select id="list" resultType="user">
          select id,name,email,age,bornDate from user
    </select>
</mapper>