<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<!-- namespace：命名空间，用于隔离sql-->
<mapper namespace="mybatis.domain.UserMapper">
    <!-- 
       keyClumn:主键对应的列
       keyProperty:主键对应的属性
       useGeneratedKeys:数据自动生成的主键id保存到oid属性当中
     -->
    <insert id="add" keyColumn="" keyProperty="" useGeneratedKeys="true" parameterType="com.gechuang.mybatis.domain.User">
          insert into user(name,email,age,bornDate) values (#{name},#{email},#{age},#{bornDate})
    </insert>
    <update id="update" parameterType="com.gechuang.mybatis.domain.User">
          update user set name = #{name},email=#{email},age=#{age},bornDate=#{bornDate} where id = #{id}
    </update>
</mapper>